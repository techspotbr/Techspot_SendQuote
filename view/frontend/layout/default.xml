<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="sendquote_page_head_components" template="Techspot_SendQuote::js/components.phtml"/>
        </referenceBlock>
        <referenceBlock name="top.links">
            <block class="Techspot\SendQuote\Block\Link" name="sendquote-link" after="my-account-link">
                <arguments>
                    <argument name="sortOrder" xsi:type="number">60</argument>
                </arguments>
            </block>
        </referenceBlock>
        <!--referenceContainer name="sidebar.additional">
            <block class="Techspot\SendQuote\Block\Customer\Sidebar" name="sendquote_sidebar" as="sendquote" template="Techspot_SendQuote::sidebar.phtml"/>
        </referenceContainer-->
    </body>
</page>
