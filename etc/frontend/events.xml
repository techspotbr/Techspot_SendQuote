<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="customer_login">
        <observer name="sendquote" instance="Techspot\SendQuote\Observer\CustomerLogin" />
    </event>
    <event name="customer_logout">
        <observer name="sendquote" instance="Techspot\SendQuote\Observer\CustomerLogout" />
    </event>
    <event name="checkout_cart_product_add_after">
        <observer name="cartaddafter" instance="Techspot\SendQuote\Observer\CartAddAfter" />
    </event>
</config>
